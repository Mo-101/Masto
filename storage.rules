rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Images and data files
    match /images/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    
    match /data/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    
    match /models/{allPaths=**} {
      allow read: if request.auth != null;
      allow write: if request.auth != null;
    }
  }
}
